<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
>
    <ui:with type="org.pavel.yanushka.client.resource.Messages" field="messages"/>
    <ui:with type="org.pavel.yanushka.client.resource.ApplicationResources" field="res"/>

    <g:FlowPanel styleName="{res.style.placesBlock}">
        <g:InlineLabel styleName="{res.style.placeText}" text="Places" ui:field="name"/>
        <g:HTMLPanel styleName="reportWizardChooseColumns" ui:field="placeImage"/>
    </g:FlowPanel>
</ui:UiBinder> 